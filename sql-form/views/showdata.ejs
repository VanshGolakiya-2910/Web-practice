<table>
    <tr>
        <td>Name</td>
        <td>Email</td>
        <td>action</td>
    </tr>
    <% for(var i=0; i < data.length; i++) { %>
    <tr>
        <td><%= data[i].name %></td>
        <td><%= data[i].email %></td>
        <td><a href="/edit/<%= data[i].id %>">Edit</a> | <a href="/delete/<%= data[i].id %>">Delete</a></td>    
    </tr>
    <% } %>
</table>